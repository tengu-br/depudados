{"ast":null,"code":"import { jsx as _jsx } from \"react/jsx-runtime\";\nimport MUIDataTable from \"mui-datatables\";\n\nvar DeputadosList = function DeputadosList(_ref) {\n  var data = _ref.data,\n      elevation = _ref.elevation;\n  var compiledData = [];\n  data.map(function (deputado) {\n    compiledData.push([deputado.nome, deputado.partido, deputado.uf, Math.round(deputado.presencaSessoes * 10000 + Number.EPSILON) / 100 + '%']);\n  });\n  var columns = [\"Nome\", \"Partido\", \"Estado\", \"Presença\"];\n  var options = {\n    selectableRows: 'none',\n    elevation: elevation,\n    textLabels: {\n      body: {\n        noMatch: \"Sem resultados compatíveis\",\n        toolTip: \"Ordenar\",\n        columnHeaderTooltip: function columnHeaderTooltip(column) {\n          return \"Ordenar por \".concat(column.label);\n        }\n      },\n      pagination: {\n        next: \"Próxima página\",\n        previous: \"Página anterior\",\n        rowsPerPage: \"Linhas por página:\",\n        displayRows: \"de\"\n      },\n      toolbar: {\n        search: \"Buscar\",\n        downloadCsv: \"Baixar CSV\",\n        print: \"Imprimir\",\n        viewColumns: \"Ver Colunas\",\n        filterTable: \"Tabela de Filtros\"\n      },\n      filter: {\n        all: \"TODOS\",\n        title: \"FILTROS\",\n        reset: \"RESETAR\"\n      },\n      viewColumns: {\n        title: \"Mostrar Colunas\",\n        titleAria: \"Mostrar/Esconder Colunas\"\n      }\n    },\n    customSort: function customSort(data, colIndex, order, meta) {\n      return data.sort(function (a, b) {\n        // Sort se for a string com valor numerico\n        if (colIndex === 3) {\n          return (parseFloat(a.data[colIndex].replace(/[^0-9-,]/g, '')) < parseFloat(b.data[colIndex].replace(/[^0-9-,%]/g, '')) ? -1 : 1) * (order === 'desc' ? 1 : -1);\n        } else {\n          return (a.data[colIndex] < b.data[colIndex] ? -1 : 1) * (order === 'desc' ? 1 : -1);\n        }\n      });\n    }\n  };\n  return /*#_